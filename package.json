{
  "name": "@us-epa-camd/easey-design-system",
  "version": "1.38.0",
  "description": "design system & component library",
  "homepage": "https://us-epa-camd.github.io/easey-design-system",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "require": "./index.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/US-EPA-CAMD/easey-design-system"
  },
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "scripts": {
    "prepare": "gulp init",
    "commit": "cz",
    "build": "gulp init && vite build",
    "test": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "gh-pages -d storybook-static",
    "semantic-release": "semantic-release",
    "postbuild": "cp -r ./package.json ./lib",
    "postversion": "cp -r package.json .."
  },
  "resolutions": {
    "ansi-html": "^0.0.8",
    "ansi-regex": "^5.0.1",
    "browserslist": "^4.16.5",
    "glob-parent": "^5.1.2",
    "immer": "^9.0.6",
    "json-schema": "^0.4.0",
    "json5": "^2.2.2",
    "loader-utils": "^2.0.4",
    "minimatch": "^3.0.5",
    "moment": "^2.29.4",
    "node-forge": "^1.3.0",
    "node-notifier": "^8.0.1",
    "nth-check": "^2.0.1",
    "object-path": "^0.11.8",
    "postcss": "^8.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-overlay": "6.0.9",
    "react-dev-utils": "^11.0.4",
    "semver": "^7.5.2",
    "serialize-javascript": "^3.1.0",
    "set-value": "^4.0.1",
    "shell-quote": "^1.7.3",
    "sockjs": "^0.3.20",
    "string-width": "4.2.3",
    "tough-cookie": "^4.1.3",
    "trim": "^0.0.3",
    "yargs-parser": "^21.1.1",
    "minimist": "^1.2.6",
    "ejs": "^3.1.7",
    "prismjs": "^1.27.0",
    "word-wrap": "^1.2.5",
    "wrap-ansi": "7.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@rollup/plugin-url": "^6.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/addon-links": "^10.2.12",
    "@storybook/builder-vite": "^10.2.8",
    "@storybook/cli": "^10.2.13",
    "@storybook/react-vite": "^10.2.12",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@trussworks/react-uswds": "^4.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.13",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@uswds/compile": "^1.3.1",
    "@uswds/uswds": "^3.13.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^30.2.0",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.27.3",
    "gh-pages": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-sonar-reporter": "2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-svg": "^14.1.1",
    "rollup-plugin-bundle-scss": "^0.1.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svg": "^2.0.0",
    "semantic-release": "^25.0.3",
    "tslib": "^2.3.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "@storybook/addon-docs": "10.2.12",
    "storybook": "^10.2.13"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "release": {
    "pkgRoot": "lib",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "lib"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "LICENSE",
            "README.md",
            "CHANGELOG.md",
            "package.json",
            "yarn.lock"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ramonak/react-progress-bar": "^4.2.0"
  }
}
